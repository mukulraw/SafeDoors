<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.technobrix.tbx.safedoors">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".bean"
        android:theme="@style/AppTheme">
        <activity android:name="com.technobrix.tbx.safedoors.MainActivity">

        </activity>
        <activity android:name="com.technobrix.tbx.safedoors.HelpDesk.HelpOne" />
        <activity android:name="com.technobrix.tbx.safedoors.SplashLogin.Login" />
        <activity android:name="com.technobrix.tbx.safedoors.SplashLogin.GetStarted" />
        <activity android:name="com.technobrix.tbx.safedoors.SplashLogin.Register" />
        <activity android:name="com.technobrix.tbx.safedoors.SplashLogin.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
    </application>

</manifest>